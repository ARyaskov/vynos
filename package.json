{
  "name": "ynos",
  "version": "0.0.1",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "build:ts": "tsc",
    "build:static": "",
    "harness:prepare": "cp -r harness/* dist/",
    "harness": "static dist"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/src/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/bignumber.js": "^4.0.2",
    "@types/jasmine": "^2.5.52",
    "gulp-cli": "^1.3.0",
    "jest": "^20.0.4",
    "node-static": "^0.7.9",
    "ts-jest": "^20.0.6",
    "typescript": "^2.3.4"
  }
}
