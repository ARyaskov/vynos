{
  "name": "ynos",
  "version": "0.0.1",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "build": "gulp build",
    "dist": "gulp dist",
    "harness": "gulp harness",
    "--harness:prepare": "gulp harness:prepare",
    "--hrness": "yarn run harness:prepare && static dist",
    "h2": "yarn run harness:prepare && static dist -p 4040"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/src/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/bignumber.js": "^4.0.2",
    "@types/jasmine": "^2.5.52",
    "babel-loader": "^7.0.0",
    "babel-preset-react": "^6.24.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.3.0",
    "gulp-serve": "^1.4.0",
    "gulp-typescript": "^3.1.7",
    "gulp-webpack": "^1.5.0",
    "jest": "^20.0.4",
    "node-static": "^0.7.9",
    "ts-jest": "^20.0.6",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1"
  }
}
